<div class="mb-5 pt-3">

    {{#if posts.length}}
    <h2>My Posts</h2>
    {{#each posts as |post|}}
</div>
{{> dash-postinfo post}}
{{/each}}

</div>


{{else}}
<h6 class="pb-4 has-text-centered is italic">You do not have any posts!</h6>

<button type="submit" class="btn"><a class="link" href="/dashboard/new">Create New Post</a></button>
<script src="/js/new-post.js"></script>
{{/if}}